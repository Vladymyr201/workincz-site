{
  "name": "workincz-site",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo 'Running tests...' && npm run test:unit && npm run test:e2e",
    "test:unit": "echo 'Running unit tests...' && exit 0",
    "test:e2e": "echo 'Running E2E tests...' && exit 0",
    "lint": "echo 'Running linter...' && exit 0",
    "build": "echo 'Building project...' && npm run build:check",
    "build:check": "echo 'Checking build files...' && node -e \"console.log('Build check passed')\"",
    "deploy": "firebase deploy",
    "deploy:staging": "firebase deploy --project staging",
    "deploy:production": "firebase deploy --project production",
    "setup": "npm install && npm run build",
    "dev": "echo 'Starting development server...' && npx serve public -p 8000",
    "start": "npm run dev"
  },
